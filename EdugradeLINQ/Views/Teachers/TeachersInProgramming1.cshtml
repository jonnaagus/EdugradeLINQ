@model IEnumerable<EdugradeLINQ.Models.Teacher>

@{
    ViewData["Title"] = "Lärare i Programmering 1";
}

<h2 class="my-4">@ViewData["Title"]</h2>

<table class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>Förnamn</th>
            <th>Efternamn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.TeacherFirstName</td>
                <td>@teacher.TeacherLastName</td>
            </tr>
        }
    </tbody>
</table>
